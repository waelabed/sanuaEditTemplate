@model IEnumerable<sauna.Models.Team>
<div class="team-section" id="ATeam" >
    <div class="container">
        <h3 class="tittle">Our Team</h3>
        <div class="team-row">
            @foreach (var item in Model)
            {

             <div class="col-md-3 team-grids">
                <div class="team-img">
                    <img width="255"height="300" class ="img-responsive" src="../images/team/@item.Img" alt="">
                    <div class="captn">
                        <ul class="top-links">
                            <li><a href="@item.FaceBookUrl"><i class="fa fa-facebook"></i></a></li>
                            <li><a href="@item.TwitterUrl"><i class="fa fa-twitter"></i></a></li>
                            <li><a href="@item.GooglePlusUrl"><i class="fa fa-google-plus"></i></a></li>
                        </ul>

                    </div>
                </div>
                <div class="team-agile">
                    <h4>@item.Name</h4>

                </div>
            </div>
          
               
               

            }
        </div>
    </div>
</div>
